#include<iostream>
using namespace std;

void findMaxFrequencyElement(char arr[],int size) {
    int freq[36]={0};
    char c;
    for(int j=0; j<size;j++){
        c=arr[j];
        if(c>='a'&&c<='z'){
            freq[c-'a'+10]++;
        }else{
            freq[c-'0']++;
        }
    }
    for(int i=0; i<36;i++){
        cout<<freq[i]<<' ';
    }
    cout<<endl;
    char max_char;
    int max=0;
    for(int i=0; i<36;i++){
            if(freq[i]>max){
                max=freq[i];
                if(i<10){
                    max_char=i+'0';
                }else{
                    max_char=(i+'a'-10);
                }
            }
    }
    cout<<max_char;
}

int main(){
    char arr[]={'a','a','a','3','9','j','3'};
    int size= sizeof(arr)/sizeof(arr[0]);
    findMaxFrequencyElement(arr,size);
    return 0;
}
